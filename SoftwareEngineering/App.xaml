<Application x:Class="SoftwareEngineering.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:SoftwareEngineering"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DesignResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <BitmapImage x:Key="LogoImage" UriSource="Resources/logo.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
